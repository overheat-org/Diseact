{
  "name": "diseact",
  "version": "4.3.1",
  "description": "A package to make Discord Embeds, Components and Commands using JSX",
  "keywords": [
    "discord.js",
    "discord",
    "react",
    "jsx",
    "embed",
    "slash",
    "command"
  ],
  "scripts": {
    "build": "node build.cjs",
    "prepublish": "npm run build",
    "test": "cls && cd test && babel-node --env-file=.env index.jsx && cd .."
  },
  "main": "cjs/index.js",
  "types": "lib/index.d.ts",
  "author": {
    "name": "</Nexus_Prime>",
    "url": "https://nexusprime.vercel.app/"
  },
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.24.8",
    "@babel/core": "^7.26.0",
    "@babel/node": "^7.25.0",
    "@babel/plugin-transform-runtime": "^7.24.7",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.24.7",
    "@babel/traverse": "^7.26.3",
    "@babel/types": "^7.26.3",
    "babel-plugin-add-import-extension": "^1.6.0",
    "canvas": "^2.11.2",
    "copyfiles": "^2.4.1",
    "discord.js": "^14.15.3",
    "dotenv": "^16.4.5",
    "esbuild": "^0.24.0"
  },
  "optionalDependencies": {
    "canvas": "^2.11.2",
    "discord.js": "^14.16.2"
  },
  "workspaces": [
    "test"
  ],
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.cjs"
    },
    "./jsx-runtime": {
      "import": "./jsx-runtime/index.mjs",
      "require": "./jsx-runtime/index.cjs"
    }
  },
  "type": "module",
  "module": "./esm/index.js",
  "dependencies": {
    "esbuild-plugin-file-path-extensions": "^2.1.4"
  }
}
