{
  "name": "diseact",
  "version": "4.2.1",
  "description": "A package to make Discord Embeds, Components and Commands using JSX",
  "keywords": [
    "discord.js",
    "discord",
    "react",
    "jsx",
    "embed",
    "slash",
    "command"
  ],
  "scripts": {
    "build": "sh ./build.sh",
    "prepublish": "npm run build",
    "test": "cls && cd test && babel-node --env-file=.env index.jsx && cd .."
  },
  "main": "cjs/index.js",
  "types": "types/lib/index.d.ts",
  "author": {
    "name": "</Nexus_Prime>",
    "url": "https://nexusprime.vercel.app/"
  },
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.24.8",
    "@babel/core": "^7.24.9",
    "@babel/node": "^7.25.0",
    "@babel/plugin-transform-runtime": "^7.24.7",
    "@babel/preset-env": "^7.25.0",
    "@babel/preset-react": "^7.24.7",
    "canvas": "^2.11.2",
    "copyfiles": "^2.4.1",
    "discord.js": "^14.15.3",
    "dotenv": "^16.4.5"
  },
  "optionalDependencies": {
    "canvas": "^2.11.2",
    "discord.js": "^14.16.2"
  },
  "workspaces": [
    "test"
  ],
  "exports": {
    ".": {
      "import": "./esm/index.js",
      "require": "./cjs/index.js"
    },
    "./jsx-runtime": {
      "import": "./esm/jsx-runtime/index.js",
      "require": "./cjs/jsx-runtime/index.js"
    }
  },
  "type": "module",
  "module": "./esm/index.js",
  "babel": {
    "env": {
      "cjs": {
        "presets": [
          [
            "@babel/preset-env",
            {
              "targets": "node 21",
              "modules": "commonjs"
            }
          ]
        ],
        "plugins": [
          [
            "@babel/plugin-transform-runtime",
            {
              "helpers": false
            }
          ]
        ]
      },
      "esm": {
        "presets": [
          [
            "@babel/preset-env",
            {
              "targets": "node 21",
              "modules": false
            }
          ]
        ],
        "plugins": [
          [
            "@babel/plugin-transform-runtime",
            {
              "helpers": false
            }
          ]
        ]
      }
    }
  }
}
